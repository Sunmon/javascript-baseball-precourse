{
    "env": {
        "browser": true,
        "node": true,
        "es2020": true
    },
    "extends": ["eslint:recommended","airbnb-base", "plugin:prettier/recommended"],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "no-continue" : "off",
        "no-plusplus" : "off",
        "no-underscore-dangle": "off"
    },
    "parser": "babel-eslint"
}
